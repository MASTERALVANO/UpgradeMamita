generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model pesanan {

  id_psn              Int      @id @default(autoincrement())
  nama_pelanggan      String
  alamat              String
  jumlah              Int
  minuman             String
  pembayaran          String
  createdAt           DateTime @default(now())
    
}